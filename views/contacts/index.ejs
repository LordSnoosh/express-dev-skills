<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Express Contacts</title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
  <h1>Contacts</h1>
  <ul>
    <% contact.forEach(function(t) { %>
      <li>
        <a href="/contacts/<%= t.id %>"><%= t.name %></a>
      </li>
    <% }); %>
  </ul>
</body>
</html>